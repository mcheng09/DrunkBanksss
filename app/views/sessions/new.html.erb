<h1> Login </h1>
<%= form_tag "/login", method: "post" do %>
<label>Username</label></br>
<%= text_field_tag :user_name, nil, required: true, class: "formInput" %></br>
<label>Password</label></br>
<%= password_field_tag :password, nil, required: true, class: "formInput" %></br>
<%= submit_tag "Login" %>
<% end %>
